    "scaffold",
    "scaffolding",
    "rapid",
    "rad"
  ],
  "author": {
    "name": "Matthew Francis Brunetti",
    "email": "zenflow87@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "find-up": "^2.1.0",
    "minimist": "^1.2.0",
    "real-time-file": "^2.0.0"
  },
  "devDependencies": {
    "execa": "^0.10.0",
    "jest": "^22.4.2",
    "recursive-copy": "^2.0.9",
    "tempy": "^0.2.1",
    "zenflow-lint-js": "^1.0.0"
  }
